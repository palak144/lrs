/******************** default css changes ***************************/

ion-alert {
    --background: #000 !important;
    .alert-wrapper {
        --background: var(--pageBackgroundColor) !important;
        .alert-head {
            padding-top: 10px;
            padding-bottom: 5px;
        }
        .alert-radio-group {
            border-top: 0px;
            padding: 0px 10px;
            border-bottom: 0px;
            border: 0px;
            // min-height: 250px !important;
            overflow-y: scroll;
            &::-webkit-scrollbar {
                display: block !important;
            }
            .alert-radio-button {
                height: 43px !important;
                .alert-radio-icon {
                    order: 1;
                    border: 2px solid #000;
                    height: 16px;
                    min-width: 16px;
                    width: 16px;
                    border-radius: 50%;
                    left: 5px;
                }
                .alert-radio-label {
                    order: 2;
                    font-size: var(--paragraph-text);
                    letter-spacing: 0.4px;
                    padding-inline-start: 15px;
                    padding-inline-end: 0px;
                    padding-left: 20px;
                    text-align: left;
                    padding-top: 5px;
                    padding-bottom: 5px;
                }
                &[aria-checked=true] {
                    .alert-radio-icon {
                        border-color: var(--MediumVioletRed);
                    }
                    .alert-radio-inner {
                        top: 2px;
                        left: 2px;
                        width: 8px;
                        height: 8px;
                        border-radius: 50%;
                        transform: none;
                        border: 0px transparent !important;
                        background: var(--MediumVioletRed);
                    }
                    .alert-radio-label {
                        color: var(--MediumVioletRed);
                    }
                }
            }
        }
        .alert-radio-group::-webkit-scrollbar {
            width: 5px;
            display: block !important;
        }
        .alert-radio-group::-webkit-scrollbar-track {
            -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
        }
        .alert-radio-group::-webkit-scrollbar-thumb {
            background-color: #000;
            outline: 1px solid slategrey;
        }
        .alert-button-group {
            display: flex;
            padding: 0px;
            margin-top: 10px;
            flex-direction: row;
            flex-flow: row;
            .alert-button {
                width: 100%;
                margin: 0px;
                border: 0px;
                background: var(--backgroundGradient);
                color: var( --font-color-white);
                font-size: var(--paragraph-text);
                letter-spacing: 0.4px;
                font-weight: var(--font-medium);
                text-align: center;
                border-radius: 0px;
                border: 0px;
                span {
                    justify-content: center;
                }
            }
        }
    }
}