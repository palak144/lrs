    --pageBackgroundColor: #fff;
    --font-color-white: #fff;
    --font-color-black: #000;
    --font-color-gray: #4d4d4d;
    --inputBorderColor: #dedede;
    --inputLeableColor: #7c7c7c;
    --MediumVioletRed: #d0196c;
    --purpleColor: #772a88;
    --pageBackgroundGry: #d0d0d0;
    --flexiLeave: #7955d1;
    --previlegeLeave: #06d1e0;
    --gryTextColor: #2a2a2a;
    --bgTransparent: transparent;
    --lightGry: #f2f2f2;
    --blackColor: #000;
    --swipMissColor: #fc204e;
    --fixedLeaveColor: #06b359;
    --workFromHomeColor: #7c38e3;
    --lightSkyBlue: #ecf2fe;
    --blueBorder: #bac5d9;
    --backgroundGradient: -webkit-linear-gradient( -45deg, #d0196c 0%, #772a88 100%);
