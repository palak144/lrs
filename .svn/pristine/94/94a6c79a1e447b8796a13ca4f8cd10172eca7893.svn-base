                    <div class="table-body" *ngIf="(_global.checkIfDataIsValid(_startFullDateManagerDashboardForHTML) && _global.checkIfDataIsValid(_endFullDateManagerDashboardForHTML)) && _global.checkIfDataIsValid(_managerDashboardData)">
                        <div class="table-row" *ngFor="let singleAssociate of _managerDashboardData.teamLeavesData | managerDashboardFilter : 'employeeName' : employeeNameFilter; let associateIndex=index;">
                            <div class="empName" ngClass="{{ singleAssociate.isDisabled }}" (click)="singleAssociate.isDisabled ? null: openEmployeeLeaveDetailsPopup(singleAssociate)">
                                {{singleAssociate.employeeName}}</div>
                            <!-- <div class="totalLeaves" (click)="openEmployeeLeaveDetailsPopup(singleAssociate)"> -->
                            <div class="totalLeaves" (click)="singleAssociate.isDisabled ? null: openEmployeeLeaveDetailsPopup(singleAssociate)">
                                {{singleAssociate.totalLeaves}}</div>
                            <div class="padding0" *ngIf="_global.checkIfDataIsValid(singleAssociate)">
                                <div class="leaveList" *ngFor="let singleMonthOfSingleAssociate of singleAssociate.monthWiseLeaves; let monthIndex=index;">
                                    <div class="month-days" ngClass="{{getLocationBasedClass()}}">
                                        <span *ngFor="let singleLeaveDayOfSingleMonth of singleMonthOfSingleAssociate.dayWiseLeavesInMonth;" class="{{singleLeaveDayOfSingleMonth.leaveType}}" [ngClass]="{'firstHalf': singleLeaveDayOfSingleMonth.halfDay =='firstHalf', 'secondHalf': singleLeaveDayOfSingleMonth.halfDay =='secondHalf' }"><i>{{singleLeaveDayOfSingleMonth.leaveType == 'normalDay' || singleLeaveDayOfSingleMonth.leaveType == 'Weekend' ? '' : singleLeaveDayOfSingleMonth.leaveType}}</i></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div *ngIf="_global.checkIfDataIsValid(_managerDashboardData) && (_managerDashboardData.ErrorCode == 3 || _managerDashboardData.ErrorCode == 1)" [innerHtml]="_managerDashboardData.ErrorMessage"></div>
        </ion-col>
    </ion-row>

    <ion-row *ngIf="_global.checkIfDataIsValid(_managerDashboardData) && _managerDashboardData.ErrorCode == 0">
        <ion-col size-xl="12" size-lg="12" size-md="12" size-sm="12" size="12" class="legendWrapper">
            <ul class="IndiaLegends" *ngIf="_locationSelectedForSearch == 'Zensar Technologies India'">
                <!-- India -->
                <li class="fixedLeaveLegend"><span></span> Fixed Holiday</li>
                <li class="optionalHolidayLegend"><span></span> Optional Holiday</li>
                <li class="flexiLeaveLegend IndiaFlexiLeaveLegend"><span></span> Flexi Leave, Privilege Leave, Other Leaves</li>
                <li class="workFromHomeLegend"><span></span> Work From Home</li>
                <li class="outOfOfficeLegend"><span></span> Out of Office</li>

            </ul>
            <ul class="USLegends" *ngIf="_locationSelectedForSearch == 'Zensar Technologies US'">
                <!-- US -->
                <li class="fixedLeaveLegend"><span></span> Fixed Holiday</li>
                <li class="floatingLeaveLegend"><span></span> Floating Leave</li>
                <li class="sickLeaveLegend usSickLeaveLegend"><span></span> Sick Leave, Vacation Leave, Other Leaves</li>
            </ul>
            <ul class="SALegends" *ngIf="_locationSelectedForSearch == 'Zensar Technologies South Africa'">
                <!-- SA -->
                <li class="annualLeaveLegend saAnnualLeaveLegend"><span></span> Annual Leave</li>
                <li class="otherLeaveLegend"><span></span> Other Leaves</li>
                <li class="fixedLeaveLegend"><span></span> Fixed Holiday</li>
                <li class="sickLeaveLegend"><span></span> Sick Leave/Advance Sick Leave</li>
            </ul>
            <ul class="UKLegends" *ngIf="_locationSelectedForSearch == 'Zensar Technologies UK'">
                <!-- UK -->
                <li class="annualLeaveLegend UKAnnualLeaveLegend"><span></span> Annual Leave</li>
                <li class="fixedLeaveLegend"><span></span> Fixed Holiday</li>
                <li class="sickLeaveLegend UKSickLeaveLegend"><span></span> Sick Leave, Other Leaves</li>
                <li class="workFromHome"><span></span> Work From Home</li>
                <li class="meetingOutOfOfficeLegend"><span></span> Meeting Out Of Office</li>
            </ul>
        </ion-col>
    </ion-row>
</ion-content>