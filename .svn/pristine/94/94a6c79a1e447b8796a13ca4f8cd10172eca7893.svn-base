                                        <span *ngIf="((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 22)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 22)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex)">22</span>
                                        <span *ngIf="((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 23)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 23)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex)">23</span>
                                        <span *ngIf="((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 24)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 24)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex)">24</span>
                                        <span *ngIf="((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 25)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 25)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex)">25</span>
                                        <span *ngIf="((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 26)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 26)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex)">26</span>
                                        <span *ngIf="((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 27)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 27)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex)">27</span>
                                        <span *ngIf="((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 28)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 28)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex)">28</span>
                                        <span *ngIf="((checkIfMonthIsInLeapYear(singleMonth.year) && singleMonth.monthId == 2) || (singleMonth.monthId != 2)) && (((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 29)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 29)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex))">29</span>
                                        <span *ngIf="(singleMonth.monthId != 2) && (((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 30)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 30)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex))">30</span>
                                        <span *ngIf="(singleMonth.monthId == 1 || singleMonth.monthId == 3 || singleMonth.monthId == 5 || singleMonth.monthId == 7 || singleMonth.monthId == 8 || singleMonth.monthId == 10 || singleMonth.monthId == 12) && (((_startFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_startFullDateManagerDashboardForHTML.getDate() <= 31)) && ((_endFullDateManagerDashboardForHTML.getMonth() == monthIndex) && (_endFullDateManagerDashboardForHTML.getDate() >= 31)) || (_startFullDateManagerDashboardForHTML.getMonth() < monthIndex && _endFullDateManagerDashboardForHTML.getMonth() > monthIndex))">31</span>
