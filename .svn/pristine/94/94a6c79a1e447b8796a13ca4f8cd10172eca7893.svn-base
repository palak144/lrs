<app-header></app-header>
<ion-content padding>
    <ion-row>
        <ion-col size-xl="12" size-lg="12" size-md="12" size-sm="12" size="12" class="managerDashboardWrapper">
            <ion-row class="inputWrapper">

                <ion-col size-xl="2" size-lg="2" size-md="2" size-sm="6" size="6" class="inputField mobileDateSec">
                    <ion-list>
                        <label>Start Date</label>
                        <ion-item (click)="openCalendarForManagerDashboard('startDateManagerDashboard')">
                            <span>{{_startDateManagerDashboard}}</span></ion-item>
                        <i class="calendarIcon" (click)="openCalendarForManagerDashboard('startDateManagerDashboard')"><img
                src="assets/imgs/calendar_gray.png" /></i>
                    </ion-list>
                </ion-col>

                <ion-col size-xl="2" size-lg="2" size-md="2" size-sm="6" size="6" class="inputField mobileDateSec">
                    <ion-list>
                        <label>End Date</label>
                        <ion-item (click)="openCalendarForManagerDashboard('endDateManagerDashboard')">
                            <span>{{_endDateManagerDashboard}}</span></ion-item>
                        <i class="calendarIcon" (click)="openCalendarForManagerDashboard('endDateManagerDashboard')"><img
                src="assets/imgs/calendar_gray.png" /></i>
                    </ion-list>
                </ion-col>

                <ion-col size-xl="2" size-lg="2" size-md="2" size-sm="9" size="9" class="inputField locationMobileView">
                    <ion-list>
                        <label>Location</label>
                        <ion-item>
                            <ion-select *ngIf="_global.checkIfDataIsValid(_managerDashboardData)" [(ngModel)]="_selectedLocationOptionManagerDashboard">
                                <ion-select-option *ngFor="let singleLocationOption of _managerDashboardData.locationList" value="{{singleLocationOption.id}}">{{singleLocationOption.value}}</ion-select-option>
                            </ion-select>
                        </ion-item>
                    </ion-list>
                </ion-col>

                <ion-col size-xl="2" size-lg="2" size-md="2" size-sm="3" size="3" class="inputField buttonMobileView">
                    <button class="goBtn" (click)="getManageboardDashboardDataForSelectedDateRange()"><img
              src="assets/icon/right-arrow.svg"></button>
                </ion-col>

