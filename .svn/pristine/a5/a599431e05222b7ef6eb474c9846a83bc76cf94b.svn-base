    }
    .col:nth-child(1),
    .col:nth-child(3) {
        max-width: 15%;
    }
    // .col:nth-child(2){
    //     max-width:25%;
    // }
    .approverInfoSection {
        background: var(--pageBackgroundGry);
        margin-top: 20px;
        border-radius: var(--roundedForImgCorner);
        p label {
            font-weight: var(--font-medium);
            font-size: var(--paragraph-text);
        }
    }
}

.mobileView,
.desktopNone {
    display: none;
}

.dateTextBox {
    .calendarIcon img {
        cursor: pointer;
    }
}

.leaveApplicationForm .buttonSection {
    text-align: right;
    width: 85%;
    display: inline-block;
}


/****************** Added CSS For Mobile View *******************/

@media (max-width: 768px) {
    .desktopView {
        display: none;
    }
    // .content-ios {
    //     .scroll-content {
    //         display: inline-block;
    //         width: 100%;
    //     }
    // }
    .scroll-content {
        background: var(--pageBackgroundGry);
        padding-bottom: 50px !important;
    }
    .pageLeftSideInfo {
        background: var(--pageBackgroundGry);
        padding: 0px 16px;
        box-shadow: none;
        display: block;
    }
    .leaveBlanceInfo {
        position: inherit;
    }
    .homepageWrapper {
        .pageLeftSideMainForm {
            order: 1;
        }
        .calendarWrapper {
            position: relative;
            order: 2;
        }
        .formWrapper {
            order: 3;
        }
    }
    .pageRightSideMain .pageRightSideInfo {
        padding: 16px;
    }
    .pageRightSideMainCalendar {
        order: 2;
        background: var(--pageBackgroundColor);
        padding: 0px 16px;
        position: static;
        -webkit-box-shadow: 0px 2px 5px 4px rgba(0, 0, 0, 0.19);
        -moz-box-shadow: 0px 2px 5px 4px rgba(0, 0, 0, 0.19);
        box-shadow: 0px 2px 5px 4px rgba(0, 0, 0, 0.19);
    }
    .pageRightSideInfoCalendar,
    .leaveApplicationForm {
        padding: 0px;
    }
    .leaveApplicationForm {
        box-shadow: none;
    }
    .actualDateBox {
        width: 100%;
        //margin-bottom: 16px;
        h2 {
            margin-bottom: 10px;
            font-size: 15px;
            margin-top: 10px !important;
        }
        div {
            padding: 16px 6px;
            width: calc(33% - 10px);
            text-align: center;
            margin-right: 10px;
            min-height: 105px;
        }
    }
    .pageLeftSideInfo .swipeMissSection {
        width: 100%;
        margin: 0 0 20px 0px;
        div {
            width: 30%;
        }
        h2 {
            margin-bottom: 10px;
        }
    }
    ion-calendar {
        padding: 10px 0px;
        padding-bottom: 0px;
    }
    .pageLeftSideMainForm {
        order: 1;
        padding: 0px;
    }
    .pageLeftSideMainForm.leaveForm {
        order: 3;
        padding: 16px;
        // margin-top: 15px;
    }
    .leaveApplicationForm .buttonSection {
        display: none;
        position: absolute;
        bottom: 0px;
        left: 0px;
        text-align: inherit;
        .button {
            margin: 0px;
            padding: 0px;
        }
        .preview,
        .clear {
            width: 49% !important;
        }
    }
    .mobileView {
        display: block;
        position: fixed;
        bottom: 0;
        z-index: 99;
        width: 100%;
        .mobileButtonMainSec {
            display: flex;
            width: 100%;
            div {
                width: 100%;
                display: flex;
                button {
                    width: 100%;
                    border-radius: 0px;
                    height: 50px;
                    margin: 0px;
                    padding: 0px;
                    box-shadow: none;
                    background: var(--backgroundGradient);
                    font-size: var(--subHeadline);
                    color: var(--font-color-white);
                }
                .clear {
                    border-left: 1px solid var(--MediumVioletRed);
                    border-right: 1px solid var(--purpleColor);
                    border-top: 1px solid var(--MediumVioletRed);
                    border-bottom: 1px solid var(--purpleColor);
                    -webkit-box-sizing: border-box;
                    -moz-box-sizing: border-box;
                    box-sizing: border-box;
                    background-position: 0 0, 0 100%;
                    background-repeat: no-repeat;
                    -webkit-background-size: 100% 1px;
                    -moz-background-size: 100% 1px;
                    background-size: 100% 1px;
                    background-image: var(--backgroundGradient);
                    background: var(--pageBackgroundGry);
                    color: var(--font-color-gray);
                }
            }
        }
    }
    .leaveApplicationForm:last-child .row {
        margin-bottom: 0px;
    }
    .approverInfoSection {
        background: var(--inputBorderColor);
        border-radius: var(--roundedForImgCorner);
        display: block;
        margin-bottom: 40px;
    }
    .mobileNone {
        display: none;
    }
    .leaveHolidyStatus {
        .leaveStatusInfo {
            border-bottom: none;
            .row {
                margin-bottom: 0px;
            }
            p {
                font-size: var(--paragraph-text-small);
                margin: 0px;
                margin-bottom: 0px;
            }
        }
    }
    .leaveHolidyStatus {
        .leaveStatusInfo .col:nth-child(1) {
            max-width: 8% !important;
        }
        span {
            width: 12px;
            height: 12px;
        }
    }
    .pageRightSideInfoCalendar ion-calendar ion-calendar-month .days-box .days .days-btn p {
        font-size: 13px !important;
    }
    ion-calendar-month button.days-btn {
        width: 26px;
        height: 26px;
    }
    ion-calendar-month .days {
        height: 26px;
    }
    .pageRightSideInfoCalendar ion-calendar .title .switch-btn .button-inner {
        font-size: 16px;
    }
}

.uploadedFileName {
    font-size: var(--paragraph-text);
    color: var(--purpleColor);
    font-weight: var(--font-semiBold);
    span {
        margin-left: 20px;
        display: inline-flex;
        flex-direction: column;
        justify-content: center;
        .icon {
            color: var(--purpleColor);
            font-size: 18px;
        }
    }
}

.calendarModal {
    .scroll-content {
        overflow-y: hidden;
        height: auto;
    }
    .title {
        .button-inner {
            font-size: var(--cardHeadlineText);
            letter-spacing: 0.4px;
            text-align: center;
            color: var(--MediumVioletRed);
            font-weight: var(--font-medium);
            text-transform: uppercase;
        }
    }
    ion-calendar-week {
        .toolbar {
            padding: 0px;
            min-height: 50px;
            .toolbar-background {
                background: var(--pageBackgroundColor);
            }
            .toolbar-content {
                .week-title {
                    color: var(--blackColor);
                    background: var(--pageBackgroundColor);
                    font-size: var(--subHeadline);
                    li {
                        color: var(--blackColor);
                        font-size: var(--subHeadline);
                    }
                }
            }
        }
    }
    .month-picker.primary {
        .month-packer-item.this-month button {
            border-color: var(--MediumVioletRed);
        }
    }
    ion-calendar-month {
        background-color: var(--pageBackgroundColor);
        .days-box {
            border-bottom: 0px;
            // .month-title{
            //     color:var(--blackColor;
            //     font-size: var(--subHeadline;
            //     letter-spacing: 0.4px;
            //     border-bottom: 20px;
            // }
            .days {
                p {
                    color: var(--blackColor);
                    font-size: var(--subHeadline);
                    letter-spacing: 0.4px;
                }
                .on-selected {
                    background: var(--MediumVioletRed);
                    border-radius: 50%;
                    p {
                        color: var(--font-color-white);
                        font-size: var(--subHeadline);
                        font-weight: var(--font-regular);
                    }
                }
                .today {
                    p {
                        color: var(--MediumVioletRed);
                        font-size: var(--subHeadline);
                        font-weight: var(--font-regular);
                    }
                }
                .today.on-selected {
                    p {
                        color: var(--font-color-white) !important;
                    }
                }
            }
        }
    }
}

.alert-md .alert-radio-group,
.alert-md .alert-checkbox-group {
    max-height: 260px !important;
}

.alert-ios .alert-radio-group,
.alert-ios .alert-checkbox-group {
    max-height: 260px !important;
}

