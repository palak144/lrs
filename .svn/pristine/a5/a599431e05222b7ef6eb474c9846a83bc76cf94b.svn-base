.scroll-content {
    background: var(--pageBackgroundColor);
    padding: 0px !important;
    margin-top: 56px;
}

.bar-button-menutoggle {
    display: block !important;
}

.col,
.grid {
    padding: 0px;
}

.leaveBlanceInfo {
    position: absolute;
    bottom: 0;
    font-size: 12px;
    .red {
        color: red;
    }
    .green {
        color: green;
    }
    .blue {
        color: blue;
    }
}

.homepageWrapper {
    padding: 0px;
    .formWrapper {
        padding: 0px;
        ion-list {
            background: transparent;
            min-height: 100px;
            ion-item {
                padding-left: 0px;
                --padding-start: 0px !important;
                --ion-safe-area-left: 0px !important;
                --padding-end: 0px !important;
                --border-width: 0px !important;
                --border-color: transparent !important;
                --ion-item-background: var(--pageBackgroundColor) !important;
                --ion-background-color: var(--pageBackgroundColor) !important;
                --inner-padding-end: 0px !important;
                --inner-padding-start: 0px !important;
                &.item-select {
                    cursor: pointer;
                }
                &:hover {
                    background: var(--pageBackgroundColor) !important;
                    --ion-item-background: var(--pageBackgroundColor) !important;
                    --ion-background-color: var(--pageBackgroundColor) !important;
                    --background-hover: var(--pageBackgroundColor) !important;
                    --background-activated: var(--pageBackgroundColor) !important;
                    --background-focused: var(--pageBackgroundColor) !important;
                    --background: var(--pageBackgroundColor) !important;
                }
                span {
                    padding-left: 15px;
                }
                &.item-textarea {
                    height: auto;
                    ion-textarea {
                        margin: 0px;
                        padding: 0px 0px 0px 15px;
                    }
                }
                 :root {
                    .item-native {
                        --padding-left: 0px;
                        .item-inner {
                            border-bottom: 0px;
                        }
                    }
                }
            }
        }
        .totalDuration {
            background: var(--pageBackgroundColor);
            height: 50px;
            border: 1px solid var(--inputBorderColor);
            border-radius: var(--roundedForImgCorner);
            padding: 0px 15px;
            display: flex;
            flex-direction: column;
            justify-content: center;
        }
        ion-select {
            max-width: 100%;
            width: 100%;
            --margin-top: 0px !important;
        }
    }
    .calendarWrapper {
        position: absolute;
        right: 0;
    }
}

.pageLeftSideMain h2,
.pageLeftSideMainForm h2,
.leaveApplicationForm h2,
.pageRightSideInfo h2,
.leaveHolidyStatus h2 {
    font-size: var(--cardHeadlineText);
    font-weight: var(--font-regular);
    margin: 0px;
    margin-bottom: 16px;
}

.actualDateBox {
    display: inline-block;
    width: 70%;
    --float: left;
    vertical-align: -webkit-baseline-middle;
    div {
        display: inline-block;
        padding: 16px;
        color: var(--font-color-white);
        text-transform: uppercase;
        font-size: var(--paragraph-text);
        font-weight: 500;
        border-radius: var(--roundedForImgCorner);
        min-width: calc(23% - 10px);
        text-align: center;
        min-height: 85px;
        float: left;
        margin-right: 10px;
        &:nth-child(2) {
            background: #00c25f;
            cursor: pointer;
        }
        &:nth-child(3) {
            background: #0081c3;
            margin-bottom: 10px;
        }
        &:nth-child(4) {
            background: #17baf1;
            margin-bottom: 10px;
        }
        &:nth-child(5) {
            background: #ff921d;
            margin-bottom: 10px;
        }
        span {
            display: block;
            text-align: center;
            font-size: var(--mainHeadlineText);
            font-weight: var(--font-regular);
            span {
                display: inline-block;
                font-size: var(--paragraph-text-small);
                font-weight: var(--font-light);
            }
        }
        i {
            display: block;
            width: 31px;
            margin: 0 auto;
        }
    }
}

.pageLeftSideInfo {
    background: var(--pageBackgroundColor);
    padding: 30px;
    order: 1;
    -webkit-box-shadow: inset -3px 0px 5px 0px rgba(0, 0, 0, 0.1);
    -moz-box-shadow: inset -3px 0px 5px 0px rgba(0, 0, 0, 0.1);
    box-shadow: inset -3px 0px 5px 0px rgba(0, 0, 0, 0.1);
    display: flex;
    .swipeMissSection {
        display: inline-block;
        width: 19%;
        margin-left: 5%;
        div {
            display: flex;
            flex-direction: column;
            justify-content: center;
            padding: 16px;
            color: #fff;
            text-transform: uppercase;
            font-size: 14px;
            font-weight: 500;
            border-radius: 4px;
            background: var(--swipMissColor);
            width: 60%;
            text-align: center;
            min-height: 85px;
            span {
                display: block;
                text-align: center;
                font-size: var(--largeMainHeadlineText);
                font-weight: var(--font-bold);
            }
        }
    }
    .pageLeftSideMainForm {
        background: var(--pageBackgroundGry);
        display: inline-block;
        padding: 0px;
    }
}

.pageLeftSideMainForm {
    background: var(--pageBackgroundGry);
    display: inline-block;
    padding: 0px;
    width: 100%;
    h2 {
        padding: 0px;
        margin: 0px;
    }
    .formWrapper {
        margin-top: 15px;
    }
    .pageRightSideMain {
        background: var(--pageBackgroundColor);
        -webkit-box-shadow: -4px 0px 23px 0px rgba(0, 0, 0, 0.09);
        -moz-box-shadow: -4px 0px 23px 0px rgba(0, 0, 0, 0.09);
        box-shadow: -4px 0px 23px 0px rgba(0, 0, 0, 0.09);
        order: 2;
        .pageRightSideInfo {
            padding: 30px;
            border-bottom: 1px solid var(--inputBorderColor);
            h2 {
                text-transform: uppercase;
            }
        }
    }
}

.pageRightSideMainCalendar {
    background: var(--pageBackgroundColor);
    // -webkit-box-shadow: -4px 0px 23px 0px rgba(0,0,0,0.09);
    // -moz-box-shadow: -4px 0px 23px 0px rgba(0,0,0,0.09);
    // box-shadow: -4px 0px 23px 0px rgba(0,0,0,0.09);
    position: absolute;
    right: 0;
}

.pageRightSideInfoCalendar {
    padding: 30px;
    order: 4;
    ion-calendar {
        .title {
            .switch-btn {
                .button-inner {
                    font-size: var(--cardHeadlineText);
                    letter-spacing: 0.4px;
                    text-transform: capitalize;
                    .arrow-dropdown {
                        font-size: 22px;
                    }
                }
            }
            .button {
                .icon {
                    font-size: 36px;
                }
            }
        }
        ion-calendar-week {
            .toolbar-content {
                .week-title {
                    li {
                        color: var(--blackColor);
                        font-size: var(--subHeadline);
                        font-weight: 500;
                    }
                }
            }
        }
        ion-calendar-month {
            .days-box {
                .days {
                    .days-btn {
                        border-radius: 50%;
                        p {
                            color: var(--blackColor);
                            font-size: var(--subHeadline);
                            letter-spacing: 0.4px;
                        }
                        &.marked {
                            background: var(--calendarOtherHoliday);
                            cursor: default;
                            p {
                                color: var(--font-color-white);
                                font-weight: var(--font-medium);
                                font-size: var(--subHeadline);
                                letter-spacing: 0.4px;
                            }
                            &.Fixed_Holiday {
                                background: var(--calendarFixedHoliday);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Floating_Leave {
                                background: var(--calendarFloatingLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Sick_Leave {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Vacation {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Bereavement_Leave {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Jury_Duty_Leave {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Leaving_Zensar_Inc {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Maternity_Leave {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Philanthropic_Work {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Paternity_Leave {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Shut_Down {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Unpaid_Leave {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.Voting_Leave {
                                background: var(--calendarAppliedLeave);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.PendingForApproval {
                                background: var(--calendarPendingApproval);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                            &.today {
                                background: var(--blackColor);
                                p {
                                    color: var(--font-color-white);
                                    font-weight: var(--font-medium);
                                    font-size: var(--subHeadline);
                                    letter-spacing: 0.4px;
                                }
                            }
                        }
                        &.on-selected {
                            background: var(--blackColor);
                            p {
                                color: var(--font-color-white);
                                font-weight: var(--font-medium);
                                font-size: var(--subHeadline);
                                letter-spacing: 0.4px;
                            }
                        }
                    }
                }
            }
        }
    }
}

.leaveApplicationForm {
    padding: 30px;
    order: 3;
    background: var(--pageBackgroundGry);
    //height: 100vh;
    -webkit-box-shadow: inset -3px 0px 5px 0px rgba(0, 0, 0, 0.1);
    -moz-box-shadow: inset -3px 0px 5px 0px rgba(0, 0, 0, 0.1);
    box-shadow: inset -3px 0px 5px 0px rgba(0, 0, 0, 0.1);
    .userCancelNote {
        margin-bottom: 15px;
        p {
            margin: 0px;
            width: 100%;
            background-color: #e1faf6;
            padding: 15px;
            border: 1px solid #9ddbce;
            border-radius: var(--roundedForImgCorner);
            font-size: var(--paragraph-text);
            letter-spacing: 0.4px;
        }
    }
    h2 {
        padding-bottom: 20px;
        border-bottom: 1px solid var(--inputBorderColor);
        position: relative;
        i {
            position: absolute;
            right: 0;
            width: 30px;
            height: 30px;
            display: inline-block;
            cursor: pointer;
        }
    }
    .row {
        margin-bottom: 20px;
    }
    .row ion-col {
        margin-right: 16px;
    }
    .item.item-input.item-input-has-focus .item-inner,
    .item.item-input.input-has-focus .item-inner {
        border-bottom-color: var(--MediumVioletRed);
        -webkit-box-shadow: inset 0 -1px 0 0 var(--MediumVioletRed);
        box-shadow: inset 0 -1px 0 0 var(--MediumVioletRed);
    }
    .list .item-input.item-input-has-focus:last-child,
    .list .item-input.input-has-focus:last-child {
        border-bottom-color: var(--MediumVioletRed);
        -webkit-box-shadow: inset 0 -1px 0 0 var(--MediumVioletRed);
        box-shadow: inset 0 -1px 0 0 var(--MediumVioletRed);
    }
    .item {
        padding-left: 5px;
        border: 1px solid var(--inputBorderColor);
        border-radius: var(--roundedForImgCorner);
        .item-inner {
            border-bottom: 0px;
        }
        .text-input {
            margin: 5px;
            font-size: var(--paragraph-text);
        }
        .select {
            width: 100%;
            max-width: 100%;
            font-size: var(--paragraph-text);
            padding: 0px;
        }
        .select-icon-inner {
            left: 0px;
            margin-top: -2px;
        }
    }
    .label,
    .label-md {
        margin: 0px;
        font-size: var(--paragraph-text);
        span {
            font-size: var(--paragraph-text);
        }
    }
    label {
        margin-bottom: 5px;
        display: inline-block;
        font-size: var(--subHeadline);
        font-weight: var(--font-regular);
    }
    .buttonSection {
        text-align: right;
        .preview,
        .clear {
            color: var(--font-color-white);
            font-size: var(--subHeadline);
            font-weight: var(--font-regular);
            height: 40px;
            border-radius: var(--roundedForImgCorner);
            text-transform: capitalize;
            width: 20%;
            box-shadow: none;
        }
        .preview {
            background: var(--backgroundGradient);
        }
        .clear {
            border-left: 1px solid var(--MediumVioletRed);
            border-right: 1px solid var(--purpleColor);
            border-top: 1px solid var(--MediumVioletRed);
            border-bottom: 1px solid var(--purpleColor);
            border-radius: var(--roundedForImgCorner);
            -webkit-box-sizing: border-box;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
            background-position: 0 0, 0 100%;
            background-repeat: no-repeat;
            -webkit-background-size: 100% 1px;
            -moz-background-size: 100% 1px;
            background-size: 100% 1px;
            background-image: var(--backgroundGradient);
            background: var(--bgTransparent);
            color: var(--font-color-gray);
            margin-right: 10px;
        }
    }
    .calendarIcon {
        position: absolute;
        top: 50px;
        right: 10px;
        cursor: pointer;
        display: flex;
    }
    .fileUpload {
        display: block;
        background: var(--pageBackgroundColor);
        padding: 10px 5px 10px 62px;
        border-radius: 3px;
        min-height: 45px;
        width: 100%;
        position: relative;
        &::after {
            content: "Upload Document";
            background: var(--backgroundGradient);
            color: #fff;
            height: 45px;
            left: 0;
            top: 0;
            position: absolute;
            padding: 0px 10px;
            display: flex;
            justify-content: center;
            width: 150px;
            text-align: center;
            flex-direction: column;
        }
    }
    .fileUpload::-webkit-file-upload-button {
        visibility: hidden;
    }
    .userComment {
        white-space: normal;
    }
}

