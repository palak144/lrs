            background: var(--calendarAppliedLeave);
        }
    }
    .Vacation {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Jury_Duty_Leave {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Leaving_Zensar_Inc {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Maternity_Leave {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Philanthropic_Work {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Paternity_Leave {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Shut_Down {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Unpaid_Leave {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Voting_Leave {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Bereavement_Leave {
        .swipMissColor {
            background: var(--calendarAppliedLeave);
        }
    }
    .Floating_Leave {
        .swipMissColor {
